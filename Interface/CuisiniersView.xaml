<Page x:Class="ProbSciANA.Interface.CuisiniersView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="CuisiniersView">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBlock Text="Gestion des Cuisiniers" FontSize="20" Margin="0,0,0,20" HorizontalAlignment="Center"/>
        <Button Style="{StaticResource MenuButtonStyle}" Content="Ajouter un cuisinier" Width="200" Margin="5"/>
        <Button Style="{StaticResource MenuButtonStyle}" Content="Modifier un cuisinier" Width="200" Margin="5"/>
        <Button Style="{StaticResource MenuButtonStyle}" Content="Supprimer un cuisinier" Width="200" Margin="5"/>
        <Button Style="{StaticResource MenuButtonStyle}" Content="Retour" Width="200"/>
    </StackPanel>
</Page>
