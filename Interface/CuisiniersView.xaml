<Page x:Class="ProbSciANA.Interface.CuisiniersView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="CuisiniersView">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBlock Text="Gestion des Cuisiniers" FontSize="20" Margin="0,0,0,20"/>
        <Button Content="Ajouter un cuisinier" Width="200" Margin="5"/>
        <Button Content="Modifier un cuisinier" Width="200" Margin="5"/>
        <Button Content="Supprimer un cuisinier" Width="200" Margin="5"/>
        <Button Content="Retour" Width="200" Margin="20,20,0,0"/>
    </StackPanel>
</Page>
